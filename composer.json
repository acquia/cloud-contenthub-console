{
    "name": "acquia/cloud-contenthub-console",
    "description": "A package providing Acquia Cloud commands for the CommonConsole command line interface.",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "require": {
        "ext-json": "*",
        "acquia/contenthub-console-helpers": "main-dev",
        "typhonius/acquia-php-sdk-v2": "^2.0.0"
    },
    "autoload": {
        "psr-4": {
            "Acquia\\Console\\Cloud\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Acquia\\Console\\Cloud\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^8",
        "squizlabs/php_codesniffer": "^3.5",
        "php-coveralls/php-coveralls": "^2.4"
    },
    "repositories": {
        "console-helpers": {
            "type": "vcs",
            "url": "git@github.com:acquia/console-helpers.git",
            "no-api": true
        }
    }
}
