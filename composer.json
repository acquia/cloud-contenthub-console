{
    "name": "acquia/cloud-console",
    "description": "A package providing Acquia Cloud commands for the CommonConsole command line interface.",
    "type": "project",
    "require": {
        "ext-json": "*",
        "eclipsegc/common-console": "0.0.x-dev",
        "acquia/console-helpers": "dev-main",
        "typhonius/acquia-php-sdk-v2": "^2.0.0"
    },
    "autoload": {
        "psr-4": {
            "Acquia\\Console\\Cloud\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Acquia\\Console\\Cloud\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^8"
    }
}
